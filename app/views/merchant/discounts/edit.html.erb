<h2>Update Discount</h2>
<%= form_for([@merchant, @discounts], url: "/merchant/discounts/#{@discounts.id}") do |f| %>
  <%= f.label :percent, 'Percent Off' %>
  <%= f.number_field :percent %>

  <%= f.label :quantity, 'Item Quantity' %>
  <%= f.number_field :quantity %>

  <%= f.submit %>
<% end %>
